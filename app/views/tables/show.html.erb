<h2> Insights</h2>

<ul id="product_table">

  <li> <%= @table.product %></li>
  <li> <%= @table.description %></li>
  <li> <%= @table.qty  %></li>
  <li> <%= @table.price %> </li>
  <li> <%= @table.total_price %> </li>
  <li> <%= @table.completed %> </li><br>

  <% if @table.completed = false %>
    <%= link_to "Edit", edit_table_path %><br>
  <% end %>
  <%= link_to "Go Back", :back %><br>
  <%= link_to "Main Menu", root_path %>
</ul>
